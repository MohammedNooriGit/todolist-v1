<%- include("header") -%>
<body>
  <div class="box" id="heading">
    <h1><%= listType %></h1>
  </div>

  <div class="box">
    <% for(var i = 0; i < items.length;i++) { %>
    <div class="item">
      <input type="checkbox" />
      <p><%= items[i] %></p>
    </div>
    <% } %>

    <form class="item" action="/" method="post">
      <input
        type="text"
        name="toDoItem"
        placeholder="New Item"
        autocomplete="off"
      />
      <button type="submit" name="list" value="<%= listType %>">+</button>
    </form>
  </div>
</body>
<%- include("footer") -%>
